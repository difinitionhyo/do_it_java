package alone;

public class Person {
	int age;
	String name;
	boolean isMarried;
	int children;
	
	public void personInfo(){
		System.out.println("이 사람의 나이 : " + age);
		System.out.println("이 사람의 이름 : " + name;)
		System.out.println("이 사람의 결혼 여부 : " + isMarried);
		System.out.println();
		
	}
}

		
