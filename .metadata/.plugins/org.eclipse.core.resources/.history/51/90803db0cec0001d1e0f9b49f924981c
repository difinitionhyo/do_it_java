package exercise;

public class P_123_Q3 {

	public static void main(String[] args) {
		//구구단을 단보다 곱하는수가 작거나 같은경우까지만 출력하는 프로그램만들기
		
		for(int dan = 1; dan < 10; dan++) {
			System.out.println(" < " + dan + " > ");
			
			
			for(int num = 1; num < 10; num++) {
				if(dan > num) {
					break;
				}
				System.out.println();
				
				
			}
			
		}
		
		
	}

}
